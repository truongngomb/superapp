# Environment Variables (copy to .env)
# =============================================================================

# API Configuration
# In development, leave empty to use Vite proxy (/api -> localhost:3001)
# In production, set to your API server URL
VITE_API_BASE_URL=

# Google OAuth (required for Google login)
VITE_GOOGLE_CLIENT_ID=

# Debug mode (optional, enabled by default in development)
VITE_ENABLE_DEBUG=false
